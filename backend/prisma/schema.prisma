generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

// Init Table
model HealthCheck {
  id        String   @id @default(cuid())
  timestamp DateTime @default(now())
}
